{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "vue/multi-word-component-names": "off",
        "no-console": "off",
        "camelcase": "off",
        "no-return-assign": "off",
        "no-empty": "off",
        "no-tabs": "off",
        "no-restricted-syntax": "off"
    }
}
